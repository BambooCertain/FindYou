/* pages/taskInfo/taskInfo.wxss */
.content {
  width: 100%;
}
.content .showMap {
  width: 100%;
  height: 60vh;
}
.content .showMap .map {
  width: 100%;
  height: 100%;
}
.content .showInfo {
  width: 100%;
  padding: 30rpx 10rpx;
}
.content .showInfo .misser_info {
  display: flex;
  height: 60px;
}
.content .showInfo .misser_info .info_pic {
  height: 60px;
}
.content .showInfo .misser_info .info_pic .misser_image {
  overflow: hidden;
  width: 60px;
  height: 60px;
}
.content .showInfo .misser_info .info_word {
  padding: 0rpx 10rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.content .showInfo .misser_info .info_word .info_misser {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.content .showInfo .misser_info .info_word .info_misser .misser {
  display: flex;
  justify-content: left;
  align-items: center;
}
.content .showInfo .misser_info .info_word .info_misser .misser .misser_name {
  font-size: 20px;
  font-weight: bolder;
  margin: 0rpx 5rpx;
}
.content .showInfo .misser_info .info_word .info_misser .misser .misser_gender {
  font-size: 15px;
  margin: 0rpx 10rpx;
  color: #ababab;
}
.content .showInfo .misser_info .info_word .info_misser .misser .misser_age {
  font-size: 15px;
  margin: 0rpx 5rpx;
  color: #ababab;
}
.content .showInfo .misser_info .info_word .info_misser .info_volunteerNum {
  display: flex;
  justify-content: right;
  align-items: center;
  font-size: 15px;
  font-weight: bolder;
  color: #006dff;
}
.content .showInfo .misser_info .info_word .info_misser .misser_address {
  display: flex;
  justify-content: left;
  align-items: center;
  font-size: 15px;
  margin: 0rpx 5rpx;
  padding-bottom: 10rpx;
}
.content .showInfo .info_nav {
  margin-top: 15px;
  display: flex;
}
.content .showInfo .info_nav .nav_btn {
  padding: 15rpx 0rpx;
  font-size: 14px;
  font-weight: bolder;
  background-color: #fff;
}
.content .showInfo .info_nav .active_btn {
  border-bottom: 5px solid #006dff;
  color: #006dff;
}
.content .showInfo .more_info {
  padding: 20rpx 10rpx 30rpx 10rpx;
}
.content .showInfo .more_info .user_info .info {
  padding: 10rpx 0rpx;
  display: flex;
  justify-content: space-between;
}
.content .showInfo .more_info .user_info .info .title {
  font-size: 13px;
}
.content .showInfo .more_info .user_info .info .value {
  font-size: 13px;
}
.content .showInfo .more_info .thread_info .nav_btn {
  padding: 20rpx 10rpx;
  display: flex;
  justify-content: space-between;
}
.content .showInfo .more_info .thread_info .nav_btn .time_chose {
  display: flex;
}
.content .showInfo .more_info .thread_info .nav_btn .time_chose .time_rank {
  width: 200rpx;
  font-size: 12px;
  border-radius: 20px;
}
.content .showInfo .more_info .thread_info .nav_btn .time_chose .time_unirank {
  width: 200rpx;
  font-size: 12px;
  border-radius: 20px;
}
.content .showInfo .more_info .thread_info .nav_btn .time_chose .time_chosen {
  border: 1px solid #000;
}
.content .showInfo .more_info .thread_info .nav_btn .create_thread .create_btn {
  width: 200rpx;
  font-size: 12px;
  border-radius: 20px;
  color: #238aff;
  border: 1px solid #238aff;
}
.content .showInfo .more_info .thread_info .thread {
  padding: 20rpx 15rpx;
  border-top: 1px solid #a2a2a2;
  border-bottom: 1px solid #a2a2a2;
}
.content .showInfo .more_info .thread_info .thread .provider {
  display: inline-block;
  font-size: 14px;
  padding-right: 30rpx;
  font-weight: bold;
}
.content .showInfo .more_info .thread_info .thread .time {
  display: inline-block;
  font-size: 10px;
  color: #a2a2a2;
}
.content .showInfo .more_info .thread_info .thread .value {
  padding-top: 20rpx;
  display: block;
  font-size: 12px;
}
