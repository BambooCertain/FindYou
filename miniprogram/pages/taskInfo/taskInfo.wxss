  width: 180rpx;
  height: 180rpx;
  border: 2px solid #d9d9d9;
}
.content .showInfo .more_info .thread_info .thread .image_content .image_box .image_list .choose_image .upload_input_box::before {
  width: 2px;
  height: 40px;
  content: " ";
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  background-color: #d9d9d9;
}
.content .showInfo .more_info .thread_info .thread .image_content .image_box .image_list .choose_image .upload_input_box::after {
  width: 40px;
  height: 2px;
